{
  "name": "web_hunt",
  "private": true,
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "esbuild app/javascript/application.js --bundle --sourcemap --outfile=app/assets/builds/application.js --loader:.js=jsx",
    "build:css": "sass app/assets/stylesheets/application.scss:app/assets/builds/application.css --no-source-map --load-path=node_modules"
  },
  "description": "Web Hunt, a Product Hunt clone, allows creators to share their products and receive feedback from tech enthusiasts.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharshi/Web-Hunt.git"
  },
  "license": "ISC",
  "dependencies": {
    "@rails/actioncable": "^8.0.0",
    "@rails/activestorage": "^8.0.0",
    "jquery": "^4.0.0",
    "lucide-react": "^0.575.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.3.4",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "esbuild": "^0.25.0",
    "redux-logger": "^3.0.6",
    "sass": "^1.83.0"
  }
}
